{
  "name": "SMOOPs_dev",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/abhaskumarrr/SMOOPs_dev",
  "author": "Abhas Kumar <109647440+abhaskumarrr@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "ml"
  ],
  "scripts": {
    "dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\" \"yarn dev:ml\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:ml": "cd ml && python main.py",
    "start": "concurrently \"yarn start:backend\" \"yarn start:frontend\" \"yarn start:ml\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:ml": "cd ml && python main.py"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
